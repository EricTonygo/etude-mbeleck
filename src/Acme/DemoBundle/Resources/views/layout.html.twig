{% extends "::base.html.twig" %}

{% block head %}
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- core CSS -->
    {% block stylesheets %}
        <link href="{{ asset('bundles/acmedemo/css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/acmedemo/css/font-awesome.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/acmedemo/css/animate.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/acmedemo/css/prettyPhoto.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/acmedemo/css/main.css') }}" rel="stylesheet">
